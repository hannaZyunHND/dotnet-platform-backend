@{
}
@using PlatformWEB.Services.Zone.ViewModal
@model List<ZoneByTreeViewMinify>

@if (Model != null)
{
    @foreach (var item in Model)
    {
        <li>
            <a href="javascript:;">
                <img class="inner-flag" src="@Utils.UIHelper.StoreFilePath(item.Icon, false)" alt="@item.Name">
                <span class="inner-label">@item.Name</span>
            </a>
        </li>
    }

}
